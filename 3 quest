#include <stdio.h>

int main() {
    float largura, altura, area;

    
    printf("Digite a largura do retangulo em metros: ");
    scanf("%f", &largura);

    printf("Digite a altura do retangulo em metros: ");
    scanf("%f", &altura);

    
    area = largura * altura;

    
    printf("A area do retangulo e: %.2f metros quadrados\n", area);

    return 0;
}
